    <artifactId>common</artifactId>
    <version>1.0.0</version>
    <packaging>jar</packaging>
    <name>Common Dependencies</name>
    <description>Common Dependencies</description>
    <url>http://study/user-server/</url>
    <organization>
        <name>com.style.study</name>
        <url>http://study.common.com</url>
    </organization>
    <developers>
        <developer>
            <id>style</id>
            <name>style</name>
            <email>343176631@qq.com</email>
            <roles>
                <role>root</role>
            </roles>
            <timezone>+8</timezone>
        </developer>
    </developers>

    <properties>
